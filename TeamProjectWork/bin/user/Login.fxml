<AnchorPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="user.userController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="1200.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../userImage/loginback.png" />
         </image>
      </ImageView>
      <Label layoutX="545.0" layoutY="86.0" text="Login">
         <font>
            <Font size="41.0" />
         </font>
      </Label>
      <Button layoutX="548.0" layoutY="464.0" mnemonicParsing="false" onAction="#loginOk" prefHeight="37.0" prefWidth="105.0" style="-fx-background-color: white; -fx-border-color: black;" text="OK">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="Sing" layoutX="872.0" layoutY="597.0" mnemonicParsing="false" onAction="#change" prefHeight="37.0" prefWidth="98.0" style="-fx-background-color: white; -fx-border-color: black;" text="Sign-up" />
      <TextField fx:id="dataId" layoutX="477.0" layoutY="267.0" prefHeight="37.0" prefWidth="352.0" />
      <PasswordField fx:id="dataPw" layoutX="479.0" layoutY="376.0" prefHeight="37.0" prefWidth="352.0" />
      <Label layoutX="382.0" layoutY="273.0" text="ID">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Label layoutX="378.0" layoutY="382.0" text="PW">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
