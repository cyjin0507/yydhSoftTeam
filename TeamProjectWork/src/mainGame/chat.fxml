<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainGame.chat">
   <children>
      <TextArea fx:id="textArea" editable="false" prefHeight="359.0" prefWidth="400.0" />
      <TextField fx:id="input" disable="true" layoutX="95.0" layoutY="359.0" maxWidth="1.7976931348623157E308" onAction="#inputAction" prefHeight="37.0" prefWidth="214.0" />
      <Button fx:id="sendButton" disable="true" layoutX="316.0" layoutY="359.0" mnemonicParsing="false" onAction="#sendButtonAction" prefHeight="37.0" prefWidth="80.0" text="보내기" />
      <Button fx:id="connectionButton" layoutX="2.0" layoutY="359.0" mnemonicParsing="false" onAction="#connectionButtonAction" prefHeight="37.0" prefWidth="92.0" text="접속하기" />
   </children>
</AnchorPane>
